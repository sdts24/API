{
  "kind": "collectionType",
  "collectionName": "miembros",
  "info": {
    "name": "Miembros"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Nombre": {
      "type": "string",
      "required": true,
      "minLength": 5
    },
    "Telefono": {
      "type": "string",
      "minLength": 10
    },
    "Correo": {
      "type": "email"
    },
    "Fecha_Inscripcion": {
      "type": "date",
      "required": true
    },
    "Estado": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "Padres": {
      "type": "string"
    }
  }
}
